@using WorkManager.Business.ViewModels.Employee;
@model List<EmployeeViewModel>
@{
    ViewData["Title"] = "Top five employees with most completed tasks for the last month.";
}

<table class="table table-striped">
    <tr>
        <th>
            Full name
        </th>
        <th>
            Email
        </th>
    </tr>
    @foreach (var employee in Model)
    {

        <tr>
            <td>
                @employee.FullName
            </td>
            <td>
                @employee.Email
            </td>
        </tr>
    }
</table>